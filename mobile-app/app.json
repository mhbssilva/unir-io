{
  "expo": {
    "name": "unir.io",
    "slug": "unir-io",
    "privacy": "public",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "facebookScheme": "PUT_FACEBOOK_SCHEME_HERE",
    "facebookAppId": "PUT_FACEBOOK_APP_ID_HERE",
    "facebookDisplayName": "unir-io",
    "splash": {
      "backgroundColor": "#009fb7"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./src/config/GoogleService-Info.plist"
    },
    "android": {
      "versionCode": 5,
      "package": "io.unir.app",
      "googleServicesFile": "./src/config/google-services.json",
      "config": {
        "googleSignIn": {
          "apiKey": "PUT_GOOGLE_SIGNIN_API_KEY_HERE",
          "certificateHash": "PUT_GOOGLE_SIGNIN_CERTIFICATE_HERE"
        }
      }
    },
    "web": {
      "config": {
        "googleSignIn": {
          "apiKey": "PUT_GOOGLE_SERVICE_API_KEY_HERE",
          "certificateHash": "PUT_GOOGLE_SERVICE_CERTIFICATE_HERE"
        },
        "firebase": {
          "apiKey": "PUT_FIREBASE_API_KEY_HERE",
          "authDomain": "PUT_FIREBASE_AUTH_DOMAIN_HERE",
          "databaseURL": "PUT_FIREBASE_DATABASE_URL_HERE",
          "projectId": "PUT_FIREBASE_PROJECT_ID_HERE",
          "storageBucket": "PUT_FIREBASE_STORAGE_BUCKET_HERE",
          "messagingSenderId": "PUT_FIREBASE_MESSAGING_SENDER_ID_HERE",
          "appId": "PUT_FIREBASE_APP_ID_HERE",
          "measurementId": "PUT_FIREBASE_MEASUREMENT_ID_HERE"
        }
      }
    },
    "description": "Academic Social Network",
    "githubUrl": "https://github.com/mhbssilva/unir-io"
  }
}
